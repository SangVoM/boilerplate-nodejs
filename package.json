{
  "name": "wino-server",
  "version": "1.0.0",
  "description": "server meeting minutes",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node --presets es2015,stage-2 ./src/app.js",
    "watch": "DEBUG=WINO nodemon ./src/app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel ./src -d dist --presets es2015,stage-2",
    "serve": "node dist/app.js",
    "seed": "./sequelizer.sh db:seed:undo:all --env=$NODE_ENV && ./sequelizer.sh db:seed:all --env=$NODE_ENV",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn format",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.14.8",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-rate-limit": "^5.3.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.12",
    "sequelize": "^6.6.5",
    "sequelize-cli": "~5.5.0",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "socket.io-redis": "^6.1.1",
    "socketio-jwt": "^4.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-stage-2": "~6.24.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.16.1"
  }
}
